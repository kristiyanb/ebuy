@using Microsoft.AspNetCore.Identity
@using EBuy.Models
@inject SignInManager<User> SignInManager
@inject UserManager<User> UserManager

<ul class="navbar-nav">
    @if (SignInManager.IsSignedIn(User))
    {
        <li class="nav-item">
            @if (User.IsInRole("Administrator") || User.IsInRole("Employee"))
            {
                <a class="nav-link btn btn-link text-dark" asp-area="Admin" asp-controller="Dashboard" asp-action="Index">Dashboard</a>
            }
            else
            {
                <div class="dropdown">
                    <button class="nav-link btn btn-link text-dark dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Shopping Cart
                    </button>
                    <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                        <div id="shoppingCart" class="dropdown-item">
                            <div id="shoppingCart-item" class="d-flex justify-content-between mt-1">
                                <img class="w-50" src="data:image/jpeg;base64," />
                                <div class="text-center w-75">
                                    <h6> HP Omen 15T</h6>
                                    <p> $1799.00</p>
                                </div>
                            </div>
                            <div id="shoppingCart-item" class="d-flex justify-content-between mt-1">
                                <img class="w-50" src="data:image/jpeg;base64," />
                                <div class="text-center w-75">
                                    <h6> HP Omen 15T</h6>
                                    <p> $1799.00</p>
                                </div>
                            </div>
                            <hr />
                            <div class="d-flex justify-content-between">
                                <h5>
                                    Total:
                                </h5>
                                <div>
                                    $3598.00
                                </div>
                            </div>
                            <button class="btn btn-danger w-100 mt-2">Buy</button>
                        </div>
                    </div>
                </div>
            }
        </li>
        <li class="nav-item">
            <a class="nav-link btn btn-link text-dark" asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage">My Profile</a>
        </li>
        <li class="nav-item">
            <form class="form-inline" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
                <button type="submit" class="nav-link btn btn-link text-dark">Logout</button>
            </form>
        </li>
    }
    else
    {
        <li class="nav-item">
            <a class="nav-link btn btn-link text-dark" asp-area="Identity" asp-page="/Account/Register">Register</a>
        </li>
        <li class="nav-item">
            <a class="nav-link btn btn-link text-dark" asp-area="Identity" asp-page="/Account/Login">Login</a>
        </li>
    }
</ul>